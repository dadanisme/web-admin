{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "blockingFunctions": {
        "triggers": [
          {
            "eventType": "beforeUserCreated",
            "function": "writeUserData"
          }
        ]
      },
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": ["npm --prefix \"$RESOURCE_DIR\" run build"]
    }
  ]
}
